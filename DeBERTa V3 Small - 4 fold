   "source": [
    "def add_inputs(df, basepath):\n",
    "    df['essay_fn'] = basepath + '/' + df.essay_id + '.txt'\n",
    "    df['inputs'] = df.discourse_type.str.lower() +' ' + df.topic_name + tokenizer.sep_token + df.discourse_text.str.lower()\n",
    "    return df"
   ]
