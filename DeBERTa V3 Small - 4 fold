    "def get_backbone_config():\n",
    "    model_config = AutoConfig.from_pretrained(config.model_name, num_labels=3)\n",
    "    model_config.hidden_dropout_prob = config.hidden_dropout_prob\n",
    "    return model_config"
   ]
